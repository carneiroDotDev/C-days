{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            // "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/bin/Debug/net9.0/HelloWorld.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            // Enable launching with a specific runtime version. Example: "5.0.9".
            // "runtimeVersion": "",
            "console": "internalConsole",
            "logging": {
                "moduleLoad": false, //This says to not log every module load
                "diagnosticsLog": {
                    "enabled": false,
                    "logFile": "${workspaceFolder}/logs/diagnostics.txt"
                }
            }
        }
    ]
}